{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "BaseDefense.csproj"
          ]
        }
      ],
      "dest": "api",
      "filter": "filter.yml",
      "includePrivateMembers": false,
      "disableGitFeatures": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "manual/**.md",
          "manual/**/toc.yml",
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadata": {
      "_appFaviconPath" : "images/icon.png",
      "_appLogoPath" : "images/logo.png",
      "_enableSearch" : true,
      "_disableContribution" : true,
      "_appFooter" : "<p>Copyright Â© 2023 Denis Dmitriev</p><p>Made with <a href=\"https://dotnet.github.io/docfx\">docfx</a></p>",
      "docurl" : "https://github.com/dducode/BaseDefense"
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "templates/custom"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": true
  }
}